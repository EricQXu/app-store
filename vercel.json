{
  "version": 2,
  "builds": [
      {
          "src": "./server.py",
          "use": "@vercel/python"
      }
  ],
  "rewrites": [
    { "source": "/static/(.*)", "destination": "/static/$1" },
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      }
  ]
}